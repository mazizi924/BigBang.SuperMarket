<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.bigbang.io/dml/globalCommand" xmlns:bb="http://www.bigbang.io/dml/globalCommand">
  <xs:element name="configuration">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="globalCommand" type="bb:DMLGlobalCommand" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="DMLGlobalCommand">
    <xs:choice>
      <xs:element name="csharp" type="bb:DMLCsharpGlobalCommand" />
      <xs:element name="storedProcedure" type="bb:DMLDatabaseGlobalCommand" />
    </xs:choice>
    <xs:attribute name="isActive" use="optional" type="xs:boolean" />
    <xs:attribute name="presentInDevelopment" use="optional" type="xs:boolean" />
    <xs:attribute name="aliasName" use="optional" type="xs:string" />
    <xs:attribute name="ignoreSuccessMessage" use="optional" type="xs:boolean" />
  </xs:complexType>
  <xs:complexType name="DMLCsharpGlobalCommand">
    <xs:all>
      <xs:element name="parametersCollection" type="bb:DMLParameterCollection" minOccurs="0" maxOccurs="1" />
      <xs:element name="confirmMessage" type="bb:DMLUserMessage" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The message that will be asked from user to confirm global command execution.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="successMessage" type="bb:DMLUserMessage" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The message that will be shown to user after the global command successfully executed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="relativeSecurityItems" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="customOperation" type="bb:DMLCustomOperationSecurityMember" />
            <xs:element name="serviceMethod" type="bb:DMLServiceMethodSecurityMember" />
            <xs:element name="service" type="bb:DMLServiceSecurityMember" />
            <xs:element name="customTask" type="bb:DMLTaskSecurityMember" />
            <xs:element name="create" type="bb:DMLEntityCreateSecurityMember" />
            <xs:element name="customCommand" type="bb:DMLEntityCustomCommandSecurityMember" />
            <xs:element name="delete" type="bb:DMLEntityDeleteSecurityMember" />
            <xs:element name="log" type="bb:DMLEntityLogSecurityMember" />
            <xs:element name="entityPrint" type="bb:DMLEntityPrintSecurityMember" />
            <xs:element name="read" type="bb:DMLEntityReadSecurityMember" />
            <xs:element name="entity" type="bb:DMLEntitySecurityTaskMember" />
            <xs:element name="stateHistory" type="bb:DMLEntityStateHistorySecurityMember" />
            <xs:element name="transition" type="bb:DMLEntityTransitionSecurityMember" />
            <xs:element name="update" type="bb:DMLEntityUpdateSecurityMember" />
            <xs:element name="form" type="bb:DMLFormSecurityMember" />
            <xs:element name="globalCommand" type="bb:DMLGlobalCommandSecurityMember" />
            <xs:element name="process" type="bb:DMLProcessSecurityMember" />
            <xs:element name="report" type="bb:DMLReportSecurityMember" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:all>
    <xs:attribute name="name" use="required" type="xs:string" />
    <xs:attribute name="displayName" use="optional" type="xs:string" />
    <xs:attribute name="serviceName" use="required" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="DMLParameterCollection">
    <xs:all>
      <xs:element name="parameters">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="parameter" type="bb:DMLParameter" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="defaultExpressions" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="defaultExpression" type="bb:DMLDefaultExpression" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="changeExpressions" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="changeExpression" type="bb:DMLChangeExpression" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="validations" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="custom" type="bb:DMLCustomValidator" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="layoutExpressions" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="readOnlyField" type="bb:DMLFieldReadOnlyLayoutExpression" />
            <xs:element name="disabledField" type="bb:DMLFieldDisabledLayoutExpression" />
            <xs:element name="invisibleField" type="bb:DMLFieldInvisibleLayoutExpression" />
            <xs:element name="readOnly" type="bb:DMLReadOnlyLayoutExpression" />
            <xs:element name="disabled" type="bb:DMLDisabledLayoutExpression" />
            <xs:element name="focusField" type="bb:DMLFieldFocusLayoutExpression" />
            <xs:element name="backgroundColorField" type="bb:DMLFieldBackgroundColorLayoutExpression" />
            <xs:element name="backgroundColor" type="bb:DMLBackgroundColorLayoutExpression" />
            <xs:element name="foreColorField" type="bb:DMLFieldForeColorLayoutExpression" />
            <xs:element name="foreColor" type="bb:DMLForeColorLayoutExpression" />
            <xs:element name="captionColorField" type="bb:DMLFieldCaptionColorLayoutExpression" />
            <xs:element name="textDecorationField" type="bb:DMLFieldTextDecorationLayoutExpression" />
            <xs:element name="textDecoration" type="bb:DMLTextDecorationLayoutExpression" />
            <xs:element name="fontStyleField" type="bb:DMLFieldFontStyleLayoutExpression" />
            <xs:element name="fontStyle" type="bb:DMLFontStyleLayoutExpression" />
            <xs:element name="filterField" type="bb:DMLFieldFilterLayoutExpression" />
            <xs:element name="selectJsonTemplateField" type="bb:DMLFieldSelectJsonSchemaTemplateLayoutExpression" />
            <xs:element name="filterEnumField" type="bb:DMLFieldEnumFilterLayoutExpression" />
            <xs:element name="crossReferenceFilterField" type="bb:DMLFieldFilterCrossReferenceLayoutExpression" />
            <xs:element name="customField" type="bb:DMLFieldCustomLayoutExpression" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:all>
    <xs:attribute name="form" use="optional" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="DMLParameter">
    <xs:all>
      <xs:element name="description" minOccurs="0" maxOccurs="1" type="xs:string" />
      <xs:element name="editor" type="bb:DMLFieldEditor" minOccurs="0" maxOccurs="1" />
      <xs:element name="parameterType" type="bb:DMLFieldDataType" minOccurs="0" maxOccurs="1" />
      <xs:element name="validations" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="notNull" type="bb:DMLNotNullFieldValidator" />
            <xs:element name="jsonSchema" type="bb:DMLJsonSchemaFieldValidator" />
            <xs:element name="binary" type="bb:DMLBinaryFieldValidator" />
            <xs:element name="string" type="bb:DMLStringFieldValidator" />
            <xs:element name="regex" type="bb:DMLRegularExpressionFieldValidator" />
            <xs:element name="custom" type="bb:DMLCustomValidator" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:all>
    <xs:attribute name="name" use="required" type="xs:string" />
    <xs:attribute name="displayName" use="optional" type="xs:string" />
    <xs:attribute name="allowNull" use="optional" type="xs:boolean" />
    <xs:attribute name="sourceType" type="bb:DMLCommandParameterSourceType" use="optional" />
    <xs:attribute name="multiplicity" type="bb:DMLCommandParameterValueMultiplicity" use="optional" />
    <xs:attribute name="order" use="optional" type="xs:int" />
    <xs:attribute name="ignoreGlobalFilter" use="optional" type="xs:boolean" />
    <xs:attribute name="isReadOnly" use="optional" type="xs:boolean" />
    <xs:attribute name="isVisible" use="optional" type="xs:boolean" />
    <xs:attribute name="logHistory" use="optional" type="xs:boolean" />
  </xs:complexType>
  <xs:simpleType name="DMLCommandParameterSourceType">
    <xs:annotation>
      <xs:documentation>Source type of parameter</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="This">
        <xs:annotation>
          <xs:documentation>Locate parameter data from the property of selected entity.(this type is valid for model command and transition)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Self">
        <xs:annotation>
          <xs:documentation>The selected entity will be assigned to parameter.(this type is valid for model command and transition)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SystemDefined">
        <xs:annotation>
          <xs:documentation>System parameter like user id or cop</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UserDefined">
        <xs:annotation>
          <xs:documentation>This type of parameter will be resolved from user entry</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DMLCommandParameterValueMultiplicity">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Single" />
      <xs:enumeration value="Multi" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="DMLFieldEditor">
    <xs:choice>
      <xs:element name="color" type="bb:DMLColorEditor" />
      <xs:element name="audio" type="bb:DMLAudioEditor" />
      <xs:element name="json" type="bb:DMLJsonEditor" />
