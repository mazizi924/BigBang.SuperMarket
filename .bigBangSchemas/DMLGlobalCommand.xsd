      <xs:element name="crossReference" type="bb:DMLCrossReferenceModelEditor" />
      <xs:element name="custom" type="bb:DMLCustomEditor" />
      <xs:element name="date" type="bb:DMLDateEditor" />
      <xs:element name="dateTime" type="bb:DMLDateTimeEditor" />
      <xs:element name="duration" type="bb:DMLDurationEditor" />
      <xs:element name="file" type="bb:DMLFileEditor" />
      <xs:element name="image" type="bb:DMLImageEditor" />
      <xs:element name="model" type="bb:DMLModelEditor" />
      <xs:element name="numerical" type="bb:DMLNumericalEditor" />
      <xs:element name="enum" type="bb:DMLEnumEditor" />
      <xs:element name="state" type="bb:DMLStateEditor" />
      <xs:element name="string" type="bb:DMLStringEditor" />
      <xs:element name="time" type="bb:DMLTimeEditor" />
    </xs:choice>
  </xs:complexType>
