      <xs:element name="numerical" type="bb:DMLNumericalEditor" />
      <xs:element name="enum" type="bb:DMLEnumEditor" />
      <xs:element name="state" type="bb:DMLStateEditor" />
      <xs:element name="string" type="bb:DMLStringEditor" />
      <xs:element name="time" type="bb:DMLTimeEditor" />
    </xs:choice>
  </xs:complexType>
